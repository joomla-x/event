{
    "name": "joomla-x/event",
    "type": "joomla-package",
    "description": "Joomla Event Package",
    "keywords": ["joomla", "framework", "event"],
    "homepage": "https://github.com/joomla-x/event",
    "license": "GPL-2.0+",
    "config": {
        "preferred-install": {
            "*": "dist"
        }
    },
    "require": {
        "php": "~7.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.3",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\Event\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\Event\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-3.0-dev": "3.0-dev"
        }
    }
}
